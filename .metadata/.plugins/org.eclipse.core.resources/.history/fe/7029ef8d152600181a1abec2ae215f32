package lession1;

import lession2.Polygon;

public class Student extends Polygon{
	private String name;
	private int age;
	public String getName() {
		var = 10;
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}
